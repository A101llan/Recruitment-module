@model HR.Web.Models.Onboarding
@{
    ViewBag.Title = "Delete Onboarding";
}
<h3>Delete Onboarding</h3>
<p>Are you sure you want to delete this onboarding?</p>
@using (Html.BeginForm("DeleteConfirmed","Onboardings", new { id = Model.Id }, FormMethod.Post)) {
    @Html.AntiForgeryToken()
    <button type="submit" class="btn btn-danger">Delete</button>
    @Html.ActionLink("Cancel","Index", null, new { @class="btn btn-link"})
}



















